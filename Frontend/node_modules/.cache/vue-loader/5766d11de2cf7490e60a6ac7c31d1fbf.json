ex) in weather\" :key=\"index\">\n            <th>\n              <p class=\"font-bold\">{{WEEKDAYS[(today + index - 1) % 7]}}</p>\n            </th>\n            <th>\n              <i v-if=\"dayReport.icon == 'rain'\" class=\"fas fa-cloud-rain\"></i> \n              <i v-if=\"dayReport.icon == 'partly-cloudy-day'\" class=\"fas fa-cloud-sun\"></i>\n              <i v-if=\"dayReport.icon == 'wind'\" class=\"fas fa-wind\"></i>\n              <i v-if=\"dayReport.icon == 'cloudy'\" class=\"fas fa-cloud\"></i>\n              <i v-if=\"dayReport.icon == 'clear-night'\" class=\"fas fa-moon\"></i>\n              <i v-if=\"dayReport.icon == 'clear-day'\" class=\"fas fa-sun\"></i>\n              <i v-if=\"dayReport.icon == 'partly-cloudy-night'\" class=\"fas fa-cloud-moon\"></i>\n              <i v-if=\"dayReport.icon == 'sleet'\" class=\"fas fa-cloud\"></i>\n              <i v-if=\"dayReport.icon == 'snow'\" class=\"fas fa-cloud-meatball\"></i>\n              <i v-if=\"dayReport.icon == 'fog'\" class=\"fas fa-smog\"></i>\n            </th>\n            \n            <th>{{dayReport.tempMin}}°</th>\n            <th>/</th>\n            <th>{{dayReport.tempMax}}°</th>\n          </tr>\n        </table>\n      </div>\n      <div class=\"w-full text-center absolute mb-16 px-12 bottom-0\">\n\t<agile :autoplay=\"true\" :autoplaySpeed=\"10000\" :slidesToShow=\"1\" v-if=\"news\">\n    \n      <div class=\"slide text-2xl\" v-for=\"newsItem in news\" :key=\"newsItem\">\n        {{ newsItem}} \n      </div>\n        </agile>\n\n\n      </div>\n\n    </div>\n\n  </div>\n</template>\n\n<script>\nconst axios = require('axios').default;\nimport { VueAgile } from 'vue-agile'\nexport default {\n  \tcomponents: {\n\t\tagile: VueAgile\n  },\n  \n  data: () => ({\n    \n    news: null,\n    weather: [],\n    calendar: [],\n    location: [],\n\n    today: new Date().getDay(),\n    WEEKDAYS: [\n      'Mo',\n      'Tu',\n      'We',\n      'Th',\n      'Fr',\n      'Sa',\n      'Su'\n    ]\n    \n  }),\n  name: 'SpectaculaireSpiegel',\n  props: {\n    msg: String,\n   \n  },\n  methods:{\n    getnews(){\n      const baseUrl = `http://${window.location.hostname}:8000`\n      console.log(baseUrl+'/news')\n\n      this.news = null\n      axios.get(baseUrl+'/news',{\n\n      }).then(response => {\n        console.log(response.data);\n        this.news = response.data;\n\n      })\n\n      axios.get(baseUrl+'/weather',{\n\n      }).then(response => {\n        console.log(response.data);\n        this.weather = response.data;\n      })\n\n            axios.get(baseUrl+'/location',{\n\n      }).then(response => {\n        console.log(response.data);\n        this.location = response.data;\n      })\n\n            axios.get(baseUrl+'/calendar',{\n\n      }).then(response => {\n        console.log(response.data);\n        this.calendar = response.data;\n      })\n\n    }\n  },\n  created(){\n        this.getnews()\n  },\n  mounted() {\n \n\n    \n\n  }\n\n}\n</script>\n\n<!-- Add \"scoped\" attribute to limit CSS to this component only -->\n"]}]}